{
  "expo": {
    "name": "react-native-chat",
    "slug": "react-native-chat",
    "privacy": "public",
    "description": "Chat with Meteor and react-native",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.luis1896.chatAntware",
      "buildNumber": "1.0.0"
    },
    "android": {
      "permissions": [
        "CAMERA_ROLL",
        "ACCESS_NETWORK_STATE"
      ],
      "package": "com.luis1896.chatAntware",
      "versionCode": 1
    },
    "githubUrl": "https://github.com/Luis-1896/reactNativeChat"
  }
}
